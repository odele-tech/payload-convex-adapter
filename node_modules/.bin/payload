#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/payload@3.73.0_bufferutil@4.1.0_graphql@16.12.0_typescript@5.9.2_utf-8-validate@6.0.6/node_modules/payload/node_modules:/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/payload@3.73.0_bufferutil@4.1.0_graphql@16.12.0_typescript@5.9.2_utf-8-validate@6.0.6/node_modules:/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/payload@3.73.0_bufferutil@4.1.0_graphql@16.12.0_typescript@5.9.2_utf-8-validate@6.0.6/node_modules/payload/node_modules:/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/payload@3.73.0_bufferutil@4.1.0_graphql@16.12.0_typescript@5.9.2_utf-8-validate@6.0.6/node_modules:/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../payload/bin.js" "$@"
else
  exec node  "$basedir/../payload/bin.js" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/tsup@8.5.1_@microsoft+api-extractor@7.56.3_@types+node@22.15.3__jiti@2.6.1_postcss@8.5.6_tsx@4.21.0_typescript@5.9.2/node_modules/tsup/dist/node_modules:/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/tsup@8.5.1_@microsoft+api-extractor@7.56.3_@types+node@22.15.3__jiti@2.6.1_postcss@8.5.6_tsx@4.21.0_typescript@5.9.2/node_modules/tsup/node_modules:/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/tsup@8.5.1_@microsoft+api-extractor@7.56.3_@types+node@22.15.3__jiti@2.6.1_postcss@8.5.6_tsx@4.21.0_typescript@5.9.2/node_modules:/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/tsup@8.5.1_@microsoft+api-extractor@7.56.3_@types+node@22.15.3__jiti@2.6.1_postcss@8.5.6_tsx@4.21.0_typescript@5.9.2/node_modules/tsup/dist/node_modules:/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/tsup@8.5.1_@microsoft+api-extractor@7.56.3_@types+node@22.15.3__jiti@2.6.1_postcss@8.5.6_tsx@4.21.0_typescript@5.9.2/node_modules/tsup/node_modules:/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/tsup@8.5.1_@microsoft+api-extractor@7.56.3_@types+node@22.15.3__jiti@2.6.1_postcss@8.5.6_tsx@4.21.0_typescript@5.9.2/node_modules:/Users/odele/Github/Odele-Tech/odele-tech-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
